{
  "expo": {
    "name": "CheckUp Direct",
    "slug": "checkup-direct",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/IMG_3726.png",
    "scheme": "checkupdirect",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0057A6" // Superman Blue
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.checkupdirect.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.checkupdirect.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0057A6" // Superman Blue
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
